function [r1,r2] = Senoide(k,f)
%% Frequencia [1,5,10,25,60,100]
w=2*pi*f;  
r1=0:0.001:10; %%retificação 1 da posição (positivo)
r2=0:0.001:10;  %%retificação 2 da posição (negativo)
i=1;

%%Position
 for t= 0:0.001:10;
     y = k*sin(w*t); %% Senoidal
     if y>0 
         r1(i)=y;  
         r2(i)=0;
     end
     if y == 0 
         r1(i)=0;  
         r2(i)=0;
     end
     if y<0 
         r2(i)=abs(y);
         r1(i)=0;
     end
     i=i+1;   
 end   


end

